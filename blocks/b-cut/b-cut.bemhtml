block b-cut {

    elem opener, default, !this.ctx._wrap: {
        local (
            this._mode='',
            this.ctx = {
                block: 'b-link',
                mods: { pseudo: 'yes' },
                content: this.ctx.content
            }
        ) this.apply()
    }

    elem content, content: [
        { elem: 'close' },
        this.ctx.content
    ]

}
